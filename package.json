{
  "name": "higher.gallery",
  "license": "MIT",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build --cache-dir=.turbo",
    "lint": "turbo run lint --cache-dir=.turbo",
    "prettier": "turbo run prettier --cache-dir=.turbo",
    "stylelint": "turbo run stylelint --cache-dir=.turbo",
    "test": "turbo run test --cache-dir=.turbo",
    "typecheck": "turbo run typecheck --cache-dir=.turbo",
    "codegen": "turbo run codegen --cache-dir=.turbo",
    "wagmi": "turbo run wagmi --cache-dir=.turbo",
    "prepare": "husky"
