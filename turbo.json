{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "web-build": {
      "dependsOn": ["codegen"]
    },
    "ponder#codegen": {
      "outputs": ["generated/schema.graphql"]
    },
    "web#codegen": {
      "dependsOn": ["ponder#codegen"],
      "outputs": ["src/generated/ponder.ts"]
    },
    "lint": {},
    "web#lint": {
      "dependsOn": ["codegen"]
    },
    "prettier": {},
    "stylelint": {},
    "test": {},
    "typecheck": {},
    "contracts#wagmi": {
      "outputs": ["out"]
    },
    "web#wagmi": {
      "dependsOn": ["contracts#wagmi"],
      "outputs": ["src/generated/wagmi.ts"]
    }
  }
}
