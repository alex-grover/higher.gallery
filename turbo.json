{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "codegen": {
      "dependsOn": ["^codegen"],
      "outputs": ["generated/*", "src/generated/ponder.ts"]
    },
    "lint": {
      "dependsOn": ["codegen"]
    },
    "prettier": {},
    "stylelint": {},
    "test": {},
    "typecheck": {},
    "contracts#wagmi": {
      "outputs": ["out"]
    },
    "web#wagmi": {
      "dependsOn": ["contracts#wagmi"],
      "outputs": ["src/generated/wagmi.ts"]
    }
  }
}
